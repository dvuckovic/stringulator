{
  "name": "stringulator",
  "version": "0.2.1",
  "description": "Quick & dirty circular string art simulator",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e --headless --quiet",
    "lint": "vue-cli-service lint",
    "pretest": "npm run lint",
    "test": "npm run test:unit && npm run test:e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dvuckovic/stringulator.git"
  },
  "keywords": [
    "string art",
    "simulator",
    "vue"
  ],
  "author": "Dusan Vuckovic <dusan@dvuckovic.com>",
  "license": "WTFPL",
  "homepage": "https://dvuckovic.com",
  "dependencies": {
    "@popperjs/core": "^2.11.0",
    "clipboard": "^2.0.6",
    "core-js": "^3.19.2",
    "hash-sum": "^2.0.0",
    "uuid": "^8.3.2",
    "vue": "^3.2.24"
  },
  "devDependencies": {
    "@dvuckovic/vue3-bootstrap-icons": "^1.0.4",
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-e2e-cypress": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "~4.5.15",
    "@vue/compiler-sfc": "^3.2.21",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^5.1.3",
    "cypress": "^9.2.0",
    "cypress-pipe": "^2.0.0",
    "cypress-plugin-snapshots": "^1.4.4",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-vue": "^8.0.3",
    "js-base64": "^3.7.2",
    "sass": "^1.45.2",
    "sass-loader": "^10.2.0",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
